<?xml version="1.0" encoding="utf-8" ?>
<Wizard>
	<Title>Droid Explorer Setup</Title>
	<Install>
		<Step>
			<Content>DroidExplorer.Bootstrapper.Panels.InstallStartPanel, DroidExplorer.Bootstrapper</Content>
			<Title></Title>
			<SubTitle></SubTitle>
			<BackEnabled>false</BackEnabled>
			
			<NextEnabled>true</NextEnabled>
			<NextText>&amp;Next</NextText>

			<CancelEnabled>true</CancelEnabled>

			<BannerVisible>false</BannerVisible>
			<DialogVisible>true</DialogVisible>

			<ContentBackColor>Window</ContentBackColor>
		</Step>

		<!--Step>
			<Content>DroidExplorer.Bootstrapper.Panels.AndroidLicensePanel, DroidExplorer.Bootstrapper</Content>
			<Title>End-User License Agreement</Title>
			<SubTitle>Please read the following license agreement carefully</SubTitle>
			<BackEnabled>true</BackEnabled>

			<NextEnabled>false</NextEnabled>
			<NextText>&amp;Next</NextText>

			<CancelEnabled>true</CancelEnabled>

			<BannerVisible>true</BannerVisible>
			<DialogVisible>false</DialogVisible>

			<ContentBackColor>Control</ContentBackColor>
		</Step-->

		<Step>
			<Content>DroidExplorer.Bootstrapper.Panels.InstallDroidExplorerPanel, DroidExplorer.Bootstrapper</Content>
			<Title>Droid Explorer Setup Wizard</Title>
			<SubTitle>Installing Droid Explorer</SubTitle>
			<BackEnabled>false</BackEnabled>

			<NextEnabled>false</NextEnabled>
			<NextText>&amp;Next</NextText>

			<CancelEnabled>false</CancelEnabled>

			<BannerVisible>true</BannerVisible>
			<DialogVisible>false</DialogVisible>

			<ContentBackColor>Control</ContentBackColor>
		</Step>

		<Step>
			<Content>DroidExplorer.Bootstrapper.Panels.RunningProcessesPanel, DroidExplorer.Bootstrapper</Content>
			<Title>Running Processes</Title>
			<SubTitle>End Processes Before Continue</SubTitle>
			<BackEnabled>false</BackEnabled>

			<NextEnabled>false</NextEnabled>
			<NextText>&amp;Next</NextText>

			<CancelEnabled>true</CancelEnabled>

			<BannerVisible>true</BannerVisible>
			<DialogVisible>false</DialogVisible>

			<ContentBackColor>Control</ContentBackColor>
		</Step>

		<Step>
			<Content>DroidExplorer.Bootstrapper.Panels.UseExistingSdkPanel, DroidExplorer.Bootstrapper</Content>
			<Title>Android SDK</Title>
			<SubTitle>Specify the path to the Android SDK.</SubTitle>
			<BackEnabled>false</BackEnabled>

			<NextEnabled>false</NextEnabled>
			<NextText>&amp;Next</NextText>

			<CancelEnabled>true</CancelEnabled>

			<BannerVisible>true</BannerVisible>
			<DialogVisible>false</DialogVisible>

			<ContentBackColor>Control</ContentBackColor>
		</Step>

		<!--Step>
			<Content>DroidExplorer.Bootstrapper.Panels.DownloadPanel, DroidExplorer.Bootstrapper</Content>
			<Title>Downloading Additional Resources</Title>
			<SubTitle>Tools that are used by Droid Explorer</SubTitle>
			<BackEnabled>false</BackEnabled>

			<NextEnabled>false</NextEnabled>
			<NextText>&amp;Next</NextText>

			<CancelEnabled>true</CancelEnabled>

			<BannerVisible>true</BannerVisible>
			<DialogVisible>false</DialogVisible>

			<ContentBackColor>Control</ContentBackColor>
		</Step-->

		<Step>
			<Content>DroidExplorer.Bootstrapper.Panels.RunningProcessesPanel, DroidExplorer.Bootstrapper</Content>
			<Title>Running Processes</Title>
			<SubTitle>End Processes Before Continue</SubTitle>
			<BackEnabled>false</BackEnabled>

			<NextEnabled>false</NextEnabled>
			<NextText>&amp;Next</NextText>

			<CancelEnabled>true</CancelEnabled>

			<BannerVisible>true</BannerVisible>
			<DialogVisible>false</DialogVisible>

			<ContentBackColor>Control</ContentBackColor>
		</Step>


		<Step>
			<Content>DroidExplorer.Bootstrapper.Panels.StartServicePanel, DroidExplorer.Bootstrapper</Content>
			<Title>Droid Explorer Service</Title>
			<SubTitle>Starting the Droid Explorer</SubTitle>
			<BackEnabled>false</BackEnabled>

			<NextEnabled>false</NextEnabled>
			<NextText>&amp;Next</NextText>

			<CancelEnabled>false</CancelEnabled>

			<BannerVisible>true</BannerVisible>
			<DialogVisible>false</DialogVisible>

			<ContentBackColor>Control</ContentBackColor>
		</Step>

		<Step>
			<Content>DroidExplorer.Bootstrapper.Panels.FinishPanel, DroidExplorer.Bootstrapper</Content>
			<Title></Title>
			<SubTitle></SubTitle>
			<BackEnabled>false</BackEnabled>

			<NextEnabled>true</NextEnabled>
			<NextText>&amp;Finish</NextText>

			<CancelEnabled>false</CancelEnabled>

			<BannerVisible>false</BannerVisible>
			<DialogVisible>true</DialogVisible>

			<ContentBackColor>Window</ContentBackColor>
		</Step>

	</Install>

	<Uninstall>
		<Step>
			<Content>DroidExplorer.Bootstrapper.Panels.UninstallStartPanel, DroidExplorer.Bootstrapper</Content>
			<Title></Title>
			<SubTitle></SubTitle>
			<BackEnabled>false</BackEnabled>

			<NextEnabled>true</NextEnabled>
			<NextText>&amp;Next</NextText>

			<CancelEnabled>true</CancelEnabled>

			<BannerVisible>false</BannerVisible>
			<DialogVisible>true</DialogVisible>

			<ContentBackColor>Window</ContentBackColor>
		</Step>

		<Step>
			<Content>DroidExplorer.Bootstrapper.Panels.RemoveSdkPanel, DroidExplorer.Bootstrapper</Content>
			<Title>Removing SDK Components</Title>
			<SubTitle>The SDK tools used by Droid Explorer</SubTitle>
			<BackEnabled>false</BackEnabled>

			<NextEnabled>false</NextEnabled>
			<NextText>&amp;Next</NextText>

			<CancelEnabled>true</CancelEnabled>

			<BannerVisible>true</BannerVisible>
			<DialogVisible>false</DialogVisible>

			<ContentBackColor>Control</ContentBackColor>
		</Step>
		
		<Step>
			<Content>DroidExplorer.Bootstrapper.Panels.UninstallDroidExplorerPanel, DroidExplorer.Bootstrapper</Content>
			<Title>Droid Explorer Setup Wizard</Title>
			<SubTitle>Installing Droid Explorer</SubTitle>
			<BackEnabled>false</BackEnabled>

			<NextEnabled>false</NextEnabled>
			<NextText>&amp;Next</NextText>

			<CancelEnabled>false</CancelEnabled>

			<BannerVisible>true</BannerVisible>
			<DialogVisible>false</DialogVisible>

			<ContentBackColor>Control</ContentBackColor>
		</Step>

		<Step>
			<Content>DroidExplorer.Bootstrapper.Panels.FinishPanel, DroidExplorer.Bootstrapper</Content>
			<Title></Title>
			<SubTitle></SubTitle>
			<BackEnabled>false</BackEnabled>

			<NextEnabled>true</NextEnabled>
			<NextText>&amp;Finish</NextText>

			<CancelEnabled>false</CancelEnabled>

			<BannerVisible>false</BannerVisible>
			<DialogVisible>true</DialogVisible>

			<ContentBackColor>Window</ContentBackColor>
		</Step>
	</Uninstall>
	<Error>
		<Content>DroidExplorer.Bootstrapper.Panels.ErrorPanel, DroidExplorer.Bootstrapper</Content>
		<Title></Title>
		<SubTitle></SubTitle>
		<BackEnabled>false</BackEnabled>

		<NextEnabled>true</NextEnabled>
		<NextText>C&amp;lose</NextText>

		<CancelEnabled>false</CancelEnabled>

		<BannerVisible>false</BannerVisible>
		<DialogVisible>true</DialogVisible>

		<ContentBackColor>Window</ContentBackColor>
	</Error>

	<Cancel>
		<Content>DroidExplorer.Bootstrapper.Panels.CancelPanel, DroidExplorer.Bootstrapper</Content>
		<Title></Title>
		<SubTitle></SubTitle>
		<BackEnabled>false</BackEnabled>

		<NextEnabled>true</NextEnabled>
		<NextText>&amp;Finish</NextText>

		<CancelEnabled>false</CancelEnabled>

		<BannerVisible>false</BannerVisible>
		<DialogVisible>true</DialogVisible>

		<ContentBackColor>Window</ContentBackColor>
	</Cancel>
</Wizard>